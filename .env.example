# ReTunnel Client Configuration
# Copy this file to .env and customize for your environment

# Server endpoint (default: localhost:6400)
# For local development, use: localhost:6400
# For production, use your server domain: api.retunnel.net
RETUNNEL_SERVER_ENDPOINT=localhost:6400

# Authentication token (optional)
# Leave empty for automatic anonymous registration
# RETUNNEL_AUTH_TOKEN=your_auth_token_here

# Log level (default: INFO)
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
RETUNNEL_LOG_LEVEL=INFO